{
  "family": "gestion-evaluaciones-task",
  "networkMode": "awsvpc",
  "taskRoleArn": "arn:aws:iam::428011609647:role/abc-task-role",
  "executionRoleArn": "arn:aws:iam::428011609647:role/abc-task-execution-role",
  "containerDefinitions": [
    {
      "name": "gestion-evaluaciones",
      "image": "428011609647.dkr.ecr.us-east-1.amazonaws.com/abc-gestion-evaluaciones:latest",
      "environment": [
        {
          "name": "AWS",
          "value": "1"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/abc/ecs/gestion-evaluaciones",
          "awslogs-create-group": "true",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [
          "curl -f localhost/health=ecs || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 0,
        "startPeriod": 0
      },
      "portMappings": [
        {
          "containerPort": 80
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
